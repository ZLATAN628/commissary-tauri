{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "commissary-tauri",
    "version": "2.0.1"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "scope": [
          "$APPDATA/**"
        ]
      },
      "all": true
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.ycx",
      "targets": "all",
      "windows": {
        "webviewInstallMode": {
          "type": "downloadBootstrapper"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://cdn.jsdelivr.net/gh/ZLATAN628/commissary-tauri@master/update.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVEQjU3RkZDMUI4QUQ1Q0MKUldUTTFZb2IvSCsxN1ZBazM4NitFby83TXNDeTFRSkxpbnBuMjA0NnY2bWQ3aUZ0SjRQU0xHYWcK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "commissary-tauri",
        "width": 590,
        "height": 790,
        "theme": "Light",
        "alwaysOnTop": true
      }
    ]
  }
}